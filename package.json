{
    "name": "backend",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "dev": "cross-env NODE_ENV=development nodemon --watch src --exec ts-node src/server.ts",
        "test:mode": "cross-env NODE_ENV=test nodemon --watch src --exec ts-node src/server.ts",
        "test": "npx dotenv -e .env.test jest --detectOpenHandles",
        "test:watch": "npx dotenv -e .env.test jest --watch",
        "test:coverage": "npx dotenv -e .env.test jest --coverage",
        "migrate": "npx prisma migrate deploy",
        "migrate:dev": "npx dotenv -e .env.development -- npx prisma migrate dev",
        "migrate:test": "npx dotenv -e .env.test -- npx prisma migrate deploy",
        "generate": "npx prisma generate"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "packageManager": "pnpm@10.12.4",
    "dependencies": {
        "@prisma/client": "^6.17.1",
        "bcryptjs": "^3.0.2",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "http-status-codes": "^2.3.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.1",
        "nodemailer": "^7.0.9",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.9",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/morgan": "^1.9.10",
        "@types/node": "^24.7.2",
        "@types/nodemailer": "^7.0.2",
        "@types/supertest": "^6.0.3",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.8",
        "cross-env": "^10.1.0",
        "dotenv-cli": "^10.0.0",
        "jest": "^30.2.0",
        "nodemon": "^3.1.10",
        "prisma": "^6.17.1",
        "supertest": "^7.1.4",
        "ts-jest": "^29.4.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3"
    }
}
